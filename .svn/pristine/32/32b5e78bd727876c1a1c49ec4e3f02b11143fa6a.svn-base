        maven { url 'https://maven.fabric.io/public' }
    }

    dependencies {
        classpath 'io.fabric.tools:gradle:1.+'
    }
}
apply plugin: 'com.android.application'
apply plugin: 'io.fabric'

repositories {
    maven { url 'https://maven.fabric.io/public' }
}

android {
    signingConfigs {
        config {
            keyAlias 'EnKlikAnketa'
            keyPassword '1kaenklikanketa'
            storeFile file('D:/AndroidProjects/1kapanel.jks')
            storePassword '1kaenklikanketa'
        }
    }
    compileSdkVersion 26
    buildToolsVersion "26.0.2"
    defaultConfig {
        applicationId "enklikanketa.com.a1kapanel"
